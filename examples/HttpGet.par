def Main: ! =
  let console = Console.Open in
  catch e => do { console.print(e).close } in ! in do {
    let try (status, headers) body = Http.Request(
      "GET",                        // method
      "https://httpbin.org/get",    // URL
      *(),                          // no headers
      [writer] writer.close(.ok!),  // empty body
    )
    body.close(.ok!).try            // not interested
    console.print(Nat.ToString(status))
    console.close
  } in !
